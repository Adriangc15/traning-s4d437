@EndUserText.label: 'Flight Travel Access Control'
@MappingRole: true
define role Z12_R_TRAVEL {
    grant 
        select
            on
                Z12_R_TRAVEL
                    where
                        (AgencyId) = aspect pfcg_auth(/LRN/AGCY, /LRN/AGCY, ACTVT = '03');
                        
}